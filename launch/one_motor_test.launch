<launch>
    <node
        name="left_wheel_pid"
        pkg="pid"
        type="controller"
        ns="left_wheel"
        respawn="true"
    />
    <node
        name="left_motor_control"
        pkg="club_bot3"
        type="motorControl_node.py"
        <!-- ns="left_wheel" /-->
        respawn="true"
        launch-prefix="xterm -e"
    />
    <node
        name="left_setpoint"
        pkg="club_bot3"
        type="setpoint_node.py"
        ns="left_wheel"
        respawn="true"
        output="screen"
    />
</launch>

<!-- rqt_plot is a resource hog, so if you're seeing high CPU usage, don't launch rqt_plot  -->
<!--node name="rqt_reconfigure" pkg="rqt_reconfigure" type="rqt_reconfigure" /-->
    <node
            pkg="rosserial_python"
            type="serial_node.py"
            name="arduino_node"
            ns="left_wheel"
            >
                <param
                    name="port"
                    value="/dev/ttyACM0"
                />
        </node>

<node name="left_wheel_pid" pkg="pid" type="controller" output="screen" >
      <param name="Kp" value="5.0" />
      <param name="Ki" value="0.0" />
      <param name="Kd" value="0.1" />
      <param name="upper_limit" value="10" />
      <param name="lower_limit" value="-10" />
      <param name="windup_limit" value="10" />
      <param name="cutoff_frequency" value="20" />
      <param name="max_loop_frequency" value="105.0" />
      <param name="min_loop_frequency" value="95.0" />
     </node>
